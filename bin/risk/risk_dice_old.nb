Cell["Calculation", "Subsubsection",
 CellChangeTimes->{{3.436117098816729*^9, 3.436117100049645*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"ZeroWin", "=", "0"}], ";", 
  RowBox[{"OneWin", "=", "0"}], ";", 
  RowBox[{"TwoWin", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"d1", "=", "1"}], ",", 
   RowBox[{"d1", "\[LessEqual]", "Sides"}], ",", 
   RowBox[{"d1", "++"}], ",", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"d2", "=", "1"}], ",", 
       RowBox[{"d2", "\[LessEqual]", "Sides"}], ",", 
       RowBox[{"d2", "++"}], ",", 
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"a1", "=", "1"}], ",", 
          RowBox[{"a1", "\[LessEqual]", "Sides"}], ",", 
          RowBox[{"a1", "++"}], ",", 
          RowBox[{"{", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"a2", "=", "1"}], ",", 
             RowBox[{"a2", "\[LessEqual]", "Sides"}], ",", 
             RowBox[{"a2", "++"}], ",", 
             RowBox[{"{", "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
               RowBox[{
                RowBox[{"a3", "=", "1"}], ",", 
                RowBox[{"a3", "\[LessEqual]", "Sides"}], ",", 
                RowBox[{"a3", "++"}], ",", 
                RowBox[{"{", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"Defense", "=", 
                   RowBox[{"Sort", "[", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"d1", ",", "d2"}], "}"}], ",", "Greater"}], 
                    "]"}]}], ";", "\[IndentingNewLine]", 
                  RowBox[{"Offense", "=", 
                   RowBox[{"Sort", "[", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"a1", ",", "a2", ",", "a3"}], "}"}], ",", 
                    "Greater"}], "]"}]}], ";", "\[IndentingNewLine]", 
                  RowBox[{"wins", "=", "0"}], ";", "\[IndentingNewLine]", 
                  RowBox[{"For", "[", 
                   RowBox[{
                    RowBox[{"i", "=", "1"}], ",", 
                    RowBox[{"i", "\[LessEqual]", "2"}], ",", 
                    RowBox[{"i", "++"}], ",", 
                    RowBox[{"{", "\[IndentingNewLine]", 
                    RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"Offense", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], ">", 
                    RowBox[{"Defense", "[", 
                    RowBox[{"[", "i", "]"}], "]"}]}], ",", 
                    RowBox[{"wins", "++"}], ",", "0"}], "]"}], ";"}], 
                    "\[IndentingNewLine]", "}"}]}], "]"}], ";", 
                  "\[IndentingNewLine]", 
                  RowBox[{"Switch", "[", 
                   RowBox[{"wins", ",", "2", ",", 
                    RowBox[{"TwoWin", "++"}], ",", "1", ",", 
                    RowBox[{"OneWin", "++"}], ",", "0", ",", 
                    RowBox[{"ZeroWin", "++"}]}], "]"}]}], 
                 "\[IndentingNewLine]", "}"}]}], "]"}], "\[IndentingNewLine]",
               "}"}]}], "]"}], "\[IndentingNewLine]", "}"}]}], "]"}], 
        "\[IndentingNewLine]", "}"}]}], "]"}], "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{
        FractionBox["100", "Sides"], " ", "d1"}], ",", "\"\<% done\>\""}], 
      "]"}]}], "\[IndentingNewLine]", "}"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TwoWinProb", "=", 
   FractionBox["TwoWin", 
    SuperscriptBox["Sides", "5"]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"OneWinProb", "=", 
   FractionBox["OneWin", 
    SuperscriptBox["Sides", "5"]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ZeroWinProb", "=", 
   FractionBox["ZeroWin", 
    SuperscriptBox["Sides", "5"]]}], ";"}]}], "Input",
 CellChangeTimes->{{3.436117024348524*^9, 3.4361170854436703`*^9}, {
