  RowBox[{"TwoWin", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"d1", "=", "1"}], ",", 
   RowBox[{"d1", "\[LessEqual]", "Sides"}], ",", 
   RowBox[{"d1", "++"}], ",", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"d2", "=", "1"}], ",", 
      RowBox[{"d2", "\[LessEqual]", "Sides"}], ",", 
      RowBox[{"d2", "++"}], ",", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"a1", "=", "1"}], ",", 
         RowBox[{"a1", "\[LessEqual]", "Sides"}], ",", 
         RowBox[{"a1", "++"}], ",", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"a2", "=", "1"}], ",", 
            RowBox[{"a2", "\[LessEqual]", "Sides"}], ",", 
            RowBox[{"a2", "++"}], ",", 
            RowBox[{"{", "\[IndentingNewLine]", 
             RowBox[{"For", "[", 
              RowBox[{
               RowBox[{"a3", "=", "1"}], ",", 
               RowBox[{"a3", "\[LessEqual]", "Sides"}], ",", 
               RowBox[{"a3", "++"}], ",", 
               RowBox[{"{", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"Defense", "=", 
                  RowBox[{"Sort", "[", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{"d1", ",", "d2"}], "}"}], ",", "Greater"}], 
                   "]"}]}], ";", "\[IndentingNewLine]", 
                 RowBox[{"Offense", "=", 
                  RowBox[{"Sort", "[", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{"a1", ",", "a2", ",", "a3"}], "}"}], ",", 
                    "Greater"}], "]"}]}], ";", "\[IndentingNewLine]", 
                 RowBox[{"wins", "=", "0"}], ";", "\[IndentingNewLine]", 
